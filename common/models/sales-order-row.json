{
  "name": "SalesOrderRow",
  "forceId": true,
  "strict": true,
  "properties": {
    "unitPrice": "number",
    "quantity": "number",
    "total": "number",
    "deleted": "boolean",
    "parentId": "string",
    "productId": "string"
  },
  "relations": {
    "parent": {
      "type": "belongsTo",
      "model": "SalesOrder",
      "foreignKey": "parentId"
    },
    "product": {
      "type": "belongsTo",
      "model": "Product",
      "foreignKey": "productId"
    }
  },
  "acls": [
    {
      "permission": "DENY",
      "principalType": "ROLE",
      "principalId": "$everyone"
    },
    {
      "permission": "ALLOW",
      "principalType": "ROLE",
      "principalId": "$authenticated"
    }
  ]
}